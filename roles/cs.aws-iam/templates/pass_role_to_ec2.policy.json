{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Stmt1496144508000",
            "Effect": "Allow",
            "Action": [
                "iam:PassRole"
            ],
            "Resource": [
                "arn:aws:iam::*:role/{{ aws_iam_name_prefix }}*",
                "arn:aws:iam::*:role/{{ aws_iam_role_app_node }}",
                "arn:aws:iam::*:role/{{ aws_iam_role_persistent_node }}"
            ]
        }
    ]
}